{
  "name": "Inventory",
  "version": "1.0.0",
  "description": "Inventory",
  "author": "MTalha",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" && eslint --fix --ext .ts .",
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register src/main.ts",
    "start:test": "cross-env NODE_ENV=test nodemon",
    "start:dev": "cross-env nodemon",
    "start:debug": "cross-env NODE_ENV=development nodemon",
    "prestart:prod": "rm -rf dist && tsc && tsc-alias && cp -r src/i18n dist/i18n",
    "start:prod": "node worker.js",
    "start:hmr": "node dist/server",
    "clear_jest": "jest --clearCache",
    "test": "cross-env NODE_ENV=test  jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit",
    "test:cov": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --coverage --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "ts-typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm"
  },
  "dependencies": {
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/typeorm": "^10.0.1",
    "@slack/web-api": "^7.2.0",
    "@types/express": "^4.17.21",
    "bull": "^4.16.0",
    "cache-manager": "^5.3.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "nest-winston": "^1.9.4",
    "nestjs-i18n": "^10.4.5",
    "nestjs-rate-limiter": "^3.1.0",
    "nestjs-typeorm-custom-repository": "^1.1.1",
    "pg": "^8.11.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.2",
    "speakeasy": "^2.0.0",
    "ts-md5": "^1.3.1",
    "typeorm": "^0.3.17",
    "typeorm-extension": "^3.1.1",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^5.3.2",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "winston-console-transport": "^1.1.9",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-elasticsearch": "^0.17.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.10",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.99",
    "@swc/jest": "^0.2.29",
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^3.15.9",
    "@types/cache-manager": "^4.0.6",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cron": "^2.0.1",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
